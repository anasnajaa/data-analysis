<%- include('../layout/header'); %>
<main>
    <div class="container">
        <div class="row">
            <div class="col-12">
                <table class="table">
                    <tbody>
                        <%data.forEach((row, index) => {%>
                            <tr>
                                <th scope="row"><%=index+1%></th>
                                <td><%=row.text%></td>
                              </tr>    
                        <%})%>
                    </tbody>
                  </table>
            </div>
        </div>
    </div>
</main>



<%- include('../layout/footer'); %>