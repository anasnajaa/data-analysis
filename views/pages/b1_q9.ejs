<%- include('../layout/header'); %>
<main>
    <div class="container">
        <div class="row">
            <div class="col-12">
                <table class="table">
                    <thead>
                    <tr>
                        <th scope="col" colspan="3">Top</th>
                    </tr>
                      <tr>
                        <th scope="col">#</th>
                        <th scope="col">country</th>
                        <th scope="col">value</th>
                      </tr>
                    </thead>
                    <tbody>
                        <%data.top.forEach((row, index) => {%>
                            <tr>
                                <th scope="row"><%=index+1%></th>
                                <td><%=row.country%></td>
                                <td><%=row.value%></td>
                            </tr>    
                        <%})%>
                    </tbody>
                  </table>
            </div>
        </div>
        <div class="row">
            <div class="col-12">
                <table class="table">
                    <thead>
                        <tr>
                            <th scope="col" colspan="3">Bottom</th>
                        </tr>
                      <tr>
                        <th scope="col">#</th>
                        <th scope="col">country</th>
                        <th scope="col">value</th>
                      </tr>
                    </thead>
                    <tbody>
                        <%data.bottom.forEach((row, index) => {%>
                            <tr>
                                <th scope="row"><%=index+1%></th>
                                <td><%=row.country%></td>
                                <td><%=row.value%></td>
                            </tr>    
                        <%})%>
                    </tbody>
                  </table>
            </div>
        </div>
    </div>
</main>



<%- include('../layout/footer'); %>