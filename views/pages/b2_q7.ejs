<%- include('../layout/header'); %>
<main>
    <div class="container">
        <div class="row">
            <div class="col-12">
                <table class="table">
                    <thead>
                        <tr>
                            <th scope="col" colspan="2">Best 3 in progress of life expectancy at age 60 for males</th>
                        </tr>
                        <tr>
                          <th scope="col">country</th>
                          <th scope="col">delta</th>
                        </tr>
                      </thead>
                    <tbody>
                        <%data.best3.forEach((row, index) => {%>
                            <tr>
                                <th scope="row"><%=row.country%></th>
                                <td><%=row.deltaSum%></td>
                              </tr>    
                        <%})%>
                    </tbody>
                  </table>
            </div>
            <div class="col-12">
                <table class="table">
                    <thead>
                        <tr>
                            <th scope="col" colspan="2">Worst 3 in progress of life expectancy at age 60 for males</th>
                        </tr>
                        <tr>
                          <th scope="col">country</th>
                          <th scope="col">delta</th>
                        </tr>
                      </thead>
                    <tbody>
                        <%data.worst3.forEach((row, index) => {%>
                            <tr>
                                <th scope="row"><%=row.country%></th>
                                <td><%=row.deltaSum%></td>
                              </tr>    
                        <%})%>
                    </tbody>
                  </table>
            </div>
        </div>
    </div>
</main>



<%- include('../layout/footer'); %>